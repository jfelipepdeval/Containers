CREATE DATABESE:BAKAMITAISTORE;

SHOW DATABASES;



CREATE TABLE dados_client(
ClientID int,
Name varchar(50)
Lastname varchar(50)
Adress varchar(150)
City varchar(50)
PostalCode int(150)
);


CREATE TABLE dados_product(
ProductID int,
Name varchar(50)
category varchar(50)
type varchar(150)
QTD int(50)
pryce int(150)
);


SHOW TABLES;


);
